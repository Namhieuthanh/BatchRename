<Window x:Class="BatchRename.NewCaseConfigDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatchRename"
        mc:Ignorable="d"
        Title="NewCaseConfigDialog" Height="200" Width="300">
    <Grid>
        <Label Content="Style: " Name="toLabel" Width="50" Height="25" Margin="61,69,181,75"/>
        <ComboBox Name="styleCombobox" Height="25" Margin="102,69,39,75">
            <ComboBoxItem IsSelected="true">
                <TextBlock Text="Upper" TextWrapping="Wrap"/>
            </ComboBoxItem>
            <ComboBoxItem>
                <TextBlock Text="Lower" TextWrapping="Wrap"/>
            </ComboBoxItem>
            <ComboBoxItem>
                <TextBlock Text="FirstUpper" TextWrapping="Wrap"/>
            </ComboBoxItem>
        </ComboBox>
        <Button Name="okButton" Width="80" Height="25" Content="OK" Click="OkButton_Click" Margin="46,124,166,20"/>
        <Button Content="Cancel" Width="80" Height="25" IsCancel="True" Margin="173,124,39,20" />
    </Grid>
</Window>
