<Window x:Class="BatchRename.PresetNameConfigDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatchRename"
        mc:Ignorable="d"
        Title="PresetNameConfigDialog" Height="200" Width="300">
    <Grid>
        <Label Content="Enter preset name:" Name="fromLabel" Height="25" Margin="91,20,91,120"/>
        <TextBox Name="presetNameTextBox" Height="25" Margin="46,71,46,73" />
        <Button Name="okButton" Width="80" Height="25" Content="OK" Click="OkButton_Click" Margin="46,124,166,20"/>
        <Button Content="Cancel" Width="80" Height="25" IsCancel="True" Margin="166,124,46,20" />
    </Grid>
</Window>
